
    test "1 is decimal 1" [
        result: trinary> "1"
        assert -> 1 = result
    ]?


    test "2 is decimal 2" [
        result: trinary> "2"
        assert -> 2 = result
    ]?

    test "10 is decimal 3" [
        result: trinary> "10"
        assert -> 3 = result
    ]?

    test "11 is decimal 4" [
        result: trinary> "11"
        assert -> 4 = result
    ]?

    test "100 is decimal 9" [
        result: trinary> "100"
        assert -> 9 = result
    ]?

    test "112 is decimal 14" [
        result: trinary> "112"
        assert -> 14 = result
    ]?

    test "222 is decimal 26" [
        result: trinary> "222"
        assert -> 26 = result
    ]?

    test "1122000120 is decimal 32091" [
        result: trinary> "1122000120"
        assert -> 32091 = result
    ]?

    test "invalid trinary is decimal 0" [
        result: trinary> "carrot"
        assert -> 0 = result
    ]?

    test "digits from 3 to 9 are invalid" [
        result: trinary> "123"
        assert -> 0 = result
    ]?

