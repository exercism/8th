
    test "List of scores" [
        result: scores [30, 50, 20, 70]
        assert -> [30, 50, 20, 70] = result
    ]?


    test "Latest score" [
        result: latest [100, 0, 90, 30]
        assert -> 30 = result
    ]?

    test "Personal best" [
        result: personal-best [40, 100, 70]
        assert -> 100 = result
    ]?

    test "Personal top three from a list of scores" [
        result: personal-top-three [10, 30, 90, 30, 100, 20, 10, 0, 30, 40, 40, 70, 70]
        assert -> [100, 90, 70] = result
    ]?

    test "Personal top highest to lowest" [
        result: personal-top-three [20, 10, 30]
        assert -> [30, 20, 10] = result
    ]?

    test "Personal top when there is a tie" [
        result: personal-top-three [40, 20, 40, 30]
        assert -> [40, 40, 30] = result
    ]?

    test "Personal top when there are less than 3" [
        result: personal-top-three [30, 70]
        assert -> [70, 30] = result
    ]?

    test "Personal top when there is only one" [
        result: personal-top-three [40]
        assert -> [40] = result
    ]?


