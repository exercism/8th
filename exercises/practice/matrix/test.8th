
    test "extract row from one number matrix" [
        result: row "1" 1
        assert -> [1] = result
    ]?


    test "can extract row" [
        result: row "1 2\n3 4" 2
        assert -> [3, 4] = result
    ]?

    test "extract row where numbers have different widths" [
        result: row "1 2\n10 20" 2
        assert -> [10, 20] = result
    ]?

    test "can extract row from non-square matrix with no corresponding column" [
        result: row "1 2 3\n4 5 6\n7 8 9\n8 7 6" 4
        assert -> [8, 7, 6] = result
    ]?

    test "extract column from one number matrix" [
        result: column "1" 1
        assert -> [1] = result
    ]?

    test "can extract column" [
        result: column "1 2 3\n4 5 6\n7 8 9" 3
        assert -> [3, 6, 9] = result
    ]?

    test "can extract column from non-square matrix with no corresponding row" [
        result: column "1 2 3 4\n5 6 7 8\n9 8 7 6" 4
        assert -> [4, 8, 6] = result
    ]?

    test "extract column where numbers have different widths" [
        result: column "89 1903 3\n18 3 1\n9 4 800" 2
        assert -> [1903, 3, 4] = result
    ]?


