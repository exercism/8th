
    test "empty" [
        result: flatten []
        assert -> [] = result
    ]?


    test "no nesting" [
        result: flatten [0, 1, 2]
        assert -> [0, 1, 2] = result
    ]?

    test "flattens a nested array" [
        result: flatten [[[]]]
        assert -> [] = result
    ]?

    test "flattens array with just integers present" [
        result: flatten [1, [2, 3, 4, 5, 6, 7], 8]
        assert -> [1, 2, 3, 4, 5, 6, 7, 8] = result
    ]?

    test "5 level nesting" [
        result: flatten [0, 2, [[2, 3], 8, 100, 4, [[[50]]]], neg 2]
        assert -> [0, 2, 2, 3, 8, 100, 4, 50, neg 2] = result
    ]?

    test "6 level nesting" [
        result: flatten [1, [2, [[3]], [4, [[5]]], 6, 7], 8]
        assert -> [1, 2, 3, 4, 5, 6, 7, 8] = result
    ]?

    test "null values are omitted from the final result" [
        result: flatten [1, 2, null]
        assert -> [1, 2] = result
    ]?

    test "consecutive null values at the front of the list are omitted from the final result" [
        result: flatten [null, null, 3]
        assert -> [3] = result
    ]?

    test "consecutive null values in the middle of the list are omitted from the final result" [
        result: flatten [1, null, null, 4]
        assert -> [1, 4] = result
    ]?

    test "6 level nest list with null values" [
        result: flatten [0, 2, [[2, 3], 8, [[100]], null, [[null]]], neg 2]
        assert -> [0, 2, 2, 3, 8, 100, neg 2] = result
    ]?

    test "all values in nested list are null" [
        result: flatten [null, [[[null]]], null, null, [[null, null], null], null]
        assert -> [] = result
    ]?


