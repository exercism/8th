    test.skip "allergic to something, but not shellfish" [
        result: allergicTo? 10 "shellfish"
        assert -> false = result
    ]

    test.skip "allergic to everything, including shellfish" [
        result: allergicTo? 255 "shellfish"
        assert -> true = result
    ]

    test.skip "not allergic to anything, including strawberries" [
        result: allergicTo? 0 "strawberries"
