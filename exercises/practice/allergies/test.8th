import {unitt}!
import {src/allergies}!

suite "Allergies" [    test.skip "not allergic to anything, including eggs" [
        result: allergicTo? 0 "eggs"
        assert -> false = result
    ]

    test.skip "allergic only to eggs" [
        result: allergicTo? 1 "eggs"
        assert -> true = result
    ]

    test.skip "allergic to eggs and something else" [
        result: allergicTo? 3 "eggs"
        assert -> true = result
    ]

    test.skip "allergic to something, but not eggs" [
        result: allergicTo? 2 "eggs"
        assert -> false = result
    ]

    test.skip "allergic to everything, including eggs" [
        result: allergicTo? 255 "eggs"
        assert -> true = result
    ]

    test.skip "not allergic to anything, including peanuts" [
        result: allergicTo? 0 "peanuts"
        assert -> false = result
    ]

    test.skip "allergic only to peanuts" [
        result: allergicTo? 2 "peanuts"
        assert -> true = result
    ]

    test.skip "allergic to peanuts and something else" [
        result: allergicTo? 7 "peanuts"
        assert -> true = result
    ]

    test.skip "allergic to something, but not peanuts" [
        result: allergicTo? 5 "peanuts"
        assert -> false = result
    ]

    test.skip "allergic to everything, including peanuts" [
        result: allergicTo? 255 "peanuts"
        assert -> true = result
    ]

    test.skip "not allergic to anything, including shellfish" [
        result: allergicTo? 0 "shellfish"
        assert -> false = result
    ]

    test.skip "allergic only to shellfish" [
        result: allergicTo? 4 "shellfish"
        assert -> true = result
