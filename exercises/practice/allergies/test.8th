    test.skip "no allergies" [
        result: list 0
        assert -> [] = result
    ]

    test.skip "just eggs" [
        result: list 1
        assert -> ["eggs"] = result
    ]

    test.skip "just peanuts" [
        result: list 2
        assert -> ["peanuts"] = result
    ]

    test.skip "just strawberries" [
        result: list 8
        assert -> ["strawberries"] = result
    ]

    test.skip "eggs and peanuts" [
        result: list 3
        assert -> ["eggs", "peanuts"] = result
    ]

    test.skip "more than eggs but not peanuts" [
        result: list 5
        assert -> ["eggs", "shellfish"] = result
    ]

    test.skip "lots of stuff" [
        result: list 248
        assert -> ["strawberries", "tomatoes", "chocolate", "pollen", "cats"] = result
    ]

    test.skip "everything" [
        result: list 255
        assert -> ["eggs", "peanuts", "shellfish", "strawberries", "tomatoes", "chocolate", "pollen", "cats"] = result
    ]

    test.skip "no allergen score parts" [
        result: list 509
        assert -> ["eggs", "shellfish", "strawberries", "tomatoes", "chocolate", "pollen", "cats"] = result
    ]

    test.skip "no allergen score parts without highest valid score" [
        result: list 257
        assert -> ["eggs"] = result
    ]
]
