    test.skip "allergic to everything, including tomatoes" [
        result: allergicTo? 255 "tomatoes"
        assert -> true = result
    ]

    test.skip "not allergic to anything, including chocolate" [
        result: allergicTo? 0 "chocolate"
        assert -> false = result
    ]

    test.skip "allergic only to chocolate" [
        result: allergicTo? 32 "chocolate"
